{
    "name": "abenevaut/laravel-settings",
    "description": "Laravel persistent settings",
    "keywords": [
        "laravel",
        "settings",
        "persistent"
    ],
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Antoine Benevaut",
            "email": "antoine@benevaut.fr"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.*",
        "doctrine/dbal": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*",
        "illuminate/database": "5.*",
        "illuminate/events": "5.*"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "abenevaut\\Settings\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    }
}
